{
    "name": "siushin/util",
    "description": "常见PHP助手函数，如：String、Array、Request、DateTime，以及PHP工具类",
    "type": "library",
    "keywords": [
        "php"
    ],
    "require": {
        "php": "^8.3",
        "ext-curl": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Siushin\\Util\\": "src/"
        },
        "files": [
            "src/Funcs/FuncArray.php",
            "src/Funcs/FuncDateTime.php",
            "src/Funcs/FuncLog.php",
            "src/Funcs/FuncRequest.php"
        ]
    },
    "authors": [
        {
            "name": "siushin",
            "email": "siushin@163.com",
            "homepage": "http://www.siushin.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "Siushin\\Util\\Installer::postInstall"
        ],
        "post-update-cmd": [
            "Siushin\\Util\\Installer::postInstall"
        ]
    }
}